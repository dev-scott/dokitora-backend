{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "confirmation_date": {
      "type": "date"
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "pharmacy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::pharmacy.pharmacy",
      "inversedBy": "order"
    },
    "date": {
      "type": "string"
    },
    "pharmacy_name": {
      "type": "string"
    },
    "order_price": {
      "type": "biginteger"
    },
    "order_pharmacy_number": {
      "type": "biginteger"
    }
  }
}
